{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "env": {
      "MY_ENV_VARIABLE": "value"
    },
    "functions": {
      "api/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  